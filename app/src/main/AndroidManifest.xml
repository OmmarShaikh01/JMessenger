<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.jabm.jabmessenger">

    <application
            android:allowBackup="true"
            android:icon="@drawable/jabmessenger_appdrawer_icon"
            android:label="@string/app_name"
            android:roundIcon="@drawable/jabmessenger_appdrawer_icon"
            android:supportsRtl="true"
            android:theme="@style/Theme.JabMessenger">
        <activity android:name=".userregisteration.GetStartedActivity">
        </activity>
        <activity
                android:name=".SplashScreenActivity"
                android:theme="@style/SplashScreenStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>